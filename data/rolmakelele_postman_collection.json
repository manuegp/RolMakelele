    {
      "name": "Socket.IO - Jugador 1",
      "description": "Eventos de Socket.IO para el primer jugador",
      "item": [
        {
          "name": "1. Conectar al Servidor",
          "description": "Establecer una conexión con el servidor Socket.IO",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "const response = pm.response.text();",
                  "const sidRegex = /\"sid\":\"([^\"]+)\"/;",
                  "const match = response.match(sidRegex);",
                  "",
                  "if (match && match[1]) {",
                  "    pm.collectionVariables.set('player1_sid', match[1]);",
                  "    console.log('Socket ID guardado: ' + match[1]);",
                  "} else {",
                  "    console.error('No se encontró el Socket ID en la respuesta');",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "http://localhost:3001/socket.io/?EIO=4&transport=polling",
              "protocol": "http",
              "host": ["localhost"],
              "port": "3001",
              "path": ["socket.io", ""],
              "query": [
                {
                  "key": "EIO",
                  "value": "4"
                },
                {
                  "key": "transport",
                  "value": "polling"
                }
              ]
            }
          },
          "response": []
        },
        {
          "name": "2. Crear Sala",
          "description": "Crear una nueva sala de juego",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "const response = pm.response.text();",
                  "const jsonStart = response.indexOf('{');",
                  "",
                  "if (jsonStart !== -1) {",
                  "    const jsonText = response.substring(jsonStart);",
                  "    try {",
                  "        const data = JSON.parse(jsonText);",
                  "        if (data.room && data.room.id) {",
                  "            pm.collectionVariables.set('room_id', data.room.id);",
                  "            console.log('Room ID guardado: ' + data.room.id);",
                  "        }",
                  "    } catch (e) {",
                  "        console.error('Error al parsear la respuesta JSON', e);",
                  "    }",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
